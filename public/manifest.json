{
  "manifest_version": 2,
  "name": "Resumogpt",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "action": {
    "default_title": "Resumogpt",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "runtime"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentScript.js"]
    }
  ]
}

